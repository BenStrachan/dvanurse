<%= simple_form_for(@claim_datum) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :client_full_name %>
    <%= f.input :provider_full_name %>
    <%= f.input :provider_id %>
    <%= f.input :date_of_service %>
    <%= f.input :service %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
